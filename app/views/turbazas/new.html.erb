<div class="row">
  <div class="span6 offset3">
    <%= form_for(@turbaza) do |f| %>

        <%= f.label :name %>
        <%= f.text_field :name %>

        <%= f.label :city_id %>
        <%= f.text_field :city_id %>

        <%= f.submit "Create new turbaza", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>